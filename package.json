{
  "name": "n8n-nodes-dart",
  "version": "0.2.4",
  "description": "The Dart n8n node",
  "license": "MIT",
  "homepage": "https://dartai.com",
  "keywords": [
    "dart",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Dart",
    "email": "software@dartai.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/its-dart/n8n-nodes-dart.git"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write . --list-different",
    "lint": "n8n-node lint",
    "lint-fix": "n8n-node lint --fix",
    "spellcheck": "cspell '**' --no-progress",
    "release": "n8n-node release",
    "req-up-all": "yarn upgrade-interactive --latest --exact",
    "prepublishOnly": "n8n-node prerelease",
    "make-api": "./admin/make-api.sh"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DartApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Dart/Dart.node.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "25.0.10",
    "cspell": "9.6.2",
    "eslint": "9.39.2",
    "n8n-workflow": "2.6.0",
    "prettier": "3.8.1",
    "release-it": "19.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
