{
  "name": "n8n-nodes-dart",
  "version": "0.1.1",
  "description": "The Dart n8n node",
  "license": "MIT",
  "homepage": "https://dartai.com",
  "keywords": [
    "dart",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Dart",
    "email": "software@dartai.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/its-dart/n8n-nodes-dart.git"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write . --list-different",
    "lint": "n8n-node lint",
    "lint-fix": "n8n-node lint --fix",
    "spellcheck": "cspell '**' --no-progress",
    "release": "n8n-node release",
    "req-up-all": "yarn upgrade-interactive --latest --exact",
    "prepublishOnly": "n8n-node prerelease",
    "make-api": "./admin/make-api.sh"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DartApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Dart/Dart.node.js"
    ]
  },
  "devDependencies": {
    "@devlikeapro/n8n-openapi-node": "0.1.4",
    "@types/node": "^20.0.0",
    "cspell": "9.0.1",
    "eslint": "9.32.0",
    "n8n-workflow": "1.82.0",
    "prettier": "3.6.2",
    "release-it": "19.0.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "@n8n/node-cli": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
